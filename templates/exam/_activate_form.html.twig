<form method="post" action="{{ path('app_exam_activate', {'id': exam.id}) }}" onsubmit="return confirm('Voulez vous activer cet examen ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('activate' ~ exam.id) }}">
    <button title="Activer pour le rendre public" class="btn badge badge-success" >Activer</button>
</form>
