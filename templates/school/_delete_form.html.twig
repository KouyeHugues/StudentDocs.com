<form method="post" action="{{ path('app_school_delete', {'id': school.id}) }}" onsubmit="return confirm('Voulez vous vraiment supprimer cette école ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ school.id) }}">
    <button  title="Supprimer du système" class="btn badge badge-danger">Supprimer</button>
</form>
